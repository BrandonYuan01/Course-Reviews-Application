<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.reviews.CourseSearchController">
   <children>
      <TextField fx:id="subject" layoutX="87.0" layoutY="14.0" />
      <TextField fx:id="courseNumber" layoutX="384.0" layoutY="14.0" />
      <Label layoutX="29.0" layoutY="18.0" text="Subject:" />
      <Label layoutX="280.0" layoutY="18.0" text="Course Number:" />
      <Label layoutX="578.0" layoutY="18.0" text="Title:" />
      <TextField fx:id="title" layoutX="611.0" layoutY="14.0" />
      <Button fx:id="search" layoutX="791.0" layoutY="14.0" mnemonicParsing="false" onMousePressed="#search" text="Search" />
      <ScrollPane hbarPolicy="NEVER" layoutY="54.0" prefHeight="667.0" prefWidth="1280.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="666.0" prefWidth="1430.0">
               <children>
                  <ListView fx:id="courseList" onMousePressed="#CourseReview" prefHeight="652.0" prefWidth="1265.0" />
               </children></AnchorPane>
        </content>
      </ScrollPane>
      <MenuButton layoutX="1202.0" layoutY="14.0" mnemonicParsing="false" text="Menu">
        <items>
          <MenuItem fx:id="myReviews" mnemonicParsing="false" onAction="#MyReviews" text="My Reviews" />
          <MenuItem fx:id="logOut" mnemonicParsing="false" onAction="#logoff" text="Log Out" />
        </items>
      </MenuButton>
      <Button layoutX="888.0" layoutY="14.0" mnemonicParsing="false" onMousePressed="#addCourse" text="Add Course" />
   </children>
</AnchorPane>
